<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sqs.projectname.server.dal.dao.TestDao">
	
	<select id="getTestInfo" resultType="TestEntry">
		select
			id, name, text, create_time, update_time 
		from 
			test 
		where
			id = #{id}
	</select>
</mapper>
